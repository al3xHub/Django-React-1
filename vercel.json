{
    "version": 2,
    "builds": [
      {
        "src": "client/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "client/build"
        }
      },
      {
        "src": "drfReactApp/drfReactApp/wsgi.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "drfReactApp/drfReactApp/wsgi.py"
      },
      {
        "src": "/(.*)",
        "dest": "/client/build/$1"
      }
    ]
  }
  